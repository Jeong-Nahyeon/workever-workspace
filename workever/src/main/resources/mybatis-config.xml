<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd" >
<configuration>

	<settings>
		<setting name="jdbcTypeForNull" value="NULL" />
	</settings>
	
	<typeAliases>
		<typeAlias type="com.workever.wk.user.model.vo.User" alias="User" />
		<typeAlias type="com.workever.wk.noticeBoard.model.vo.NoticeBoard" alias="NoticeBoard" />
		<typeAlias type="com.workever.wk.deptBoard.model.vo.DeptBoard" alias="DeptBoard" />
		<typeAlias type="com.workever.wk.anonymousBoard.model.vo.AnonymousBoard" alias="AnonymousBoard" />
		<typeAlias type="com.workever.wk.community.model.vo.CommunityReply" alias="CommunityReply" />
		<typeAlias type="com.workever.wk.community.model.vo.CommunityFiles" alias="CommunityFiles" />
		<typeAlias type="com.workever.wk.report.model.vo.Report" alias="Report" />
		<typeAlias type="com.workever.wk.approval.model.vo.Approval" alias="Approval" />
		<typeAlias type="com.workever.wk.approval.model.vo.ApprovalBuisnessTripForm" alias="ApprovalBuisnessTripForm" />
		<typeAlias type="com.workever.wk.approval.model.vo.ApprovalDayOffForm" alias="ApprovalDayOffForm" />
		<typeAlias type="com.workever.wk.approval.model.vo.ApprovalExpenseReportForm" alias="ApprovalExpenseReportForm" />
		<typeAlias type="com.workever.wk.approval.model.vo.ApprovalOverTimeForm" alias="ApprovalOverTimeForm" />
		<typeAlias type="com.workever.wk.approval.model.vo.ApprovalWorkReportForm" alias="ApprovalWorkReportForm" />
		<typeAlias type="com.workever.wk.approval.model.vo.ApprovalForm" alias="ApprovalForm" />
		<typeAlias type="com.workever.wk.approval.model.vo.ApprovalLine" alias="ApprovalLine" />
		<typeAlias type="com.workever.wk.user.model.vo.Company" alias="Company"/>
	</typeAliases>
	
	<mappers>
		<mapper resource="/mappers/user-mapper.xml" />
		<mapper resource="/mappers/noticeBoard-mapper.xml" />
		<mapper resource="/mappers/deptBoard-mapper.xml" />
		<mapper resource="/mappers/anonymousBoard-mapper.xml" />
		<mapper resource="/mappers/communityReply-mapper.xml" />
		<mapper resource="/mappers/report-mapper.xml" />
		<mapper resource="/mappers/approval-mapper.xml" />
	</mappers>

</configuration>